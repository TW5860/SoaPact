{
    "provider": {
        "name": "Country-Data-Provider"
    },
    "consumer": {
        "name": "Country-Consumer"
    },
    "interactions": [
        {
            "description": "A request for an existing country",
            "request": {
                "method": "POST",
                "path": "/",
                "body": {
                    "Je#getCountryRequest": {
                        "Je#name": "Spain"
                    },
                    "#xmlns": {
                        "Je": "http://spring.io/guides/gs-producing-web-service"
                    }
                }
            },
            "response": {
                "status": 200,
                "body": {
                    "Je#getCountryResponse": {
                        "Je#country": {
                            "Je#name": "Spain",
                            "Je#population": 100
                        }
                    },
                    "#xmlns": {
                        "Je": "http://spring.io/guides/gs-producing-web-service"
                    }
                },
                "matchingRules": {
                    "body": {
                        "$['Je#getCountryResponse']['Je#country']['Je#population']": {
                            "matchers": [
                                {
                                    "match": "integer"
                                }
                            ],
                            "combine": "AND"
                        }
                    }
                },
                "generators": {
                    "body": {
                        "$['Je#getCountryResponse']['Je#country']['Je#population']": {
                            "type": "RandomInt",
                            "min": 0,
                            "max": 2147483647
                        }
                    }
                }
            },
            "providerStates": [
                {
                    "name": "provider is available"
                }
            ]
        }
    ],
    "metadata": {
        "pact-specification": {
            "version": "3.0.0"
        },
        "pact-jvm": {
            "version": "3.5.8"
        }
    }
}