{
    "provider": {
        "name": "Country-Data-Provider"
    },
    "consumer": {
        "name": "Country-Consumer"
    },
    "interactions": [
        {
            "description": "A request for an existing country",
            "request": {
                "method": "POST",
                "path": "/",
                "body": {
                    "Je#getCountryRequest": {
                        "Je#name": "Spain"
                    },
                    "#xmlns": {
                        "Je": "http://spring.io/guides/gs-producing-web-service"
                    }
                }
            },
            "response": {
                "status": 200,
                "body": {
                    "Je#getCountryResponse": {
                        "Je#country": {
                            "Je#name": "Spain",
                            "Je#capital": "Madrid"
                        }
                    },
                    "#xmlns": {
                        "Je": "http://spring.io/guides/gs-producing-web-service"
                    }
                }
            },
            "providerStates": [
                {
                    "name": "provider is available"
                }
            ]
        }
    ],
    "metadata": {
        "pact-specification": {
            "version": "3.0.0"
        },
        "pact-jvm": {
            "version": "3.5.10"
        }
    }
}